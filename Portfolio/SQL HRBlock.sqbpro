CREATE TABLE Suppliers (
    SupplierID INTEGER PRIMARY KEY,
    SupplierName TEXT NOT NULL,
    Category TEXT,
    RiskLevel TEXT, -- Low, Medium, High
    DiversityStatus TEXT, -- Certified, Not Certified
    PaymentTerms TEXT
);

-- 2. Create Contracts Table
CREATE TABLE Contracts (
    ContractID INTEGER PRIMARY KEY,
    SupplierID INTEGER,
    Category TEXT,
    StartDate DATE,
    EndDate DATE,
    ContractValue REAL,
    SavingsTarget REAL,
    IsActive TEXT, -- Yes, No
    FOREIGN KEY (SupplierID) REFERENCES Suppliers(SupplierID)
);

-- 3. Create PurchaseOrders Table
CREATE TABLE PurchaseOrders (
    PO_ID INTEGER PRIMARY KEY,
    SupplierID INTEGER,
    OrderDate DATE,
    DeliveryDate DATE,
    ActualDeliveryDate DATE,
    Category TEXT,
    Amount REAL,
    Status TEXT, -- Delivered, Pending, Delayed
    FOREIGN KEY (SupplierID) REFERENCES Suppliers(SupplierID)
);

-- 4. Create Spend Table (Detailed transactional data)
CREATE TABLE Spend (
    TransactionID INTEGER PRIMARY KEY,
    Date DATE,
    SupplierID INTEGER,
    Category TEXT,
    Description TEXT,
    Amount REAL,
    CostCenter TEXT, -- e.g., &quot;Field Office&quot;, &quot;Marketing&quot;, &quot;IT&quot;
    FOREIGN KEY (SupplierID) REFERENCES Suppliers(SupplierID)
);

